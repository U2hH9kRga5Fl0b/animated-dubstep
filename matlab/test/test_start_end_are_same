function [is_valid] = test_start_end_are_same(c, sol)
is_valid = true;
for d=1:c.number_of_drivers
    for a=2:c.number_of_actions
    if sol(d,a) <= 0
         continue
    end
    if sol(d,a+1) <= 0
        continue
    end
        
    if sol(d,a+1) > 0
    a=a+1;
    end
    start = c.start_location
        % Size dumpster entering next stop
    ending = c.actions(sol(d,a)).location;
        if  start ~= ending
            is_valid = false;
        end
    end
end
